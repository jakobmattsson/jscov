{
  "name": "jscov",
  "description": "JSCoverage, implemented in JavaScript",
  "version": "0.6.12",
  "author": "Jakob Mattsson <jakob.mattsson@gmail.com> (jakobmattsson.se)",
  "main": "./lib/jscov.js",
  "bin": {
    "jscov": "./bin/jscov.js"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/jakobmattsson/jscov"
  },
  "engines": {
    "npm": ">= 1.3 < 2",
    "node": ">= 0.10 < 0.11"
  },
  "scripts": {
    "test": "mocha --timeout 20000 --compilers coffee:coffee-script spec/*.coffee",
    "test-coverage": "npm run pretest && sh scripts/run-jscoverage.sh lib .cov && SRC_DIR=.cov mocha --compilers coffee:coffee-script --reporter mocha-term-cov-reporter --recursive spec",
    "pretest": "coffee -co lib src && sh scripts/pretest.sh",
    "prepublish": "coffee -co lib src"
  },
  "dependencies": {
    "async": "^0.2.9",
    "coffee-script": "^1.4.0",
    "wrench": "^1.4.0",
    "optimist": ">= 0.3.5 < 0.7",
    "underscore": "^1.4.0",
    "esprima": "^1.0.0",
    "escodegen": "^0.0.15"
  },
  "devDependencies": {
    "mocha-term-cov-reporter": "^0.2.0",
    "should": "^1.2.1",
    "mocha": "^1.8.1"
  }
}
